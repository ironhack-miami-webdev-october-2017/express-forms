<form method="get" action="/process-search">
  <label for="search-input"> Search Term </label>
  <input  id="search-input" type="search" placeholder="e.g. hot dog"
      name="myTerm" value="<%= theSearchTerm %>" />
  <br />

  <input  id="check-input" type="checkbox" name="hotDogCheck" />
  <label for="check-input"> Interested in hot dogs? </label>
  <br />

  <button> Search </button>
</form>

